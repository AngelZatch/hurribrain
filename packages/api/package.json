{
  "name": "api",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "private": true,
  "scripts": {
    "start": "tsc-watch --onSuccess \"node dist/index.js\"",
    "build": "tsc",
    "lint": "eslint src/",
    "format": "prettier --write **.*{json,yml,yaml,js,ts} && yarn run lint --fix --quiet",
    "mikro-orm": "NODE_OPTIONS='--loader ts-node/esm --no-warnings' mikro-orm",
    "database:migrate": "yarn mikro-orm migration:up",
    "database:seed": "yarn mikro-orm seeder:run",
    "database:rebuild": "yarn mikro-orm migration:fresh --seed",
    "test": "ts-mocha -p tsconfig.json --file src/utils/setupTests.ts src/**/*.test.ts --exit"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@faker-js/faker": "^9.0.3",
    "@mikro-orm/cli": "^6.3.13",
    "@types/eslint__js": "^8.42.3",
    "@types/expect.js": "^0.3.32",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.7.5",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "c8": "^10.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "expect.js": "^0.3.1",
    "mocha": "^10.8.2",
    "prettier": "^3.3.3",
    "sinon": "^19.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.1.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@mikro-orm/core": "^6.3.13",
    "@mikro-orm/migrations": "^6.3.13",
    "@mikro-orm/mysql": "^6.3.13",
    "@mikro-orm/reflection": "^6.3.13",
    "@mikro-orm/seeder": "^6.3.13",
    "@sinclair/typebox": "^0.33.15",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "pino": "^9.4.0",
    "uuid": "^10.0.0"
  },
  "type": "module"
}